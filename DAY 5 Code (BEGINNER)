// https://www.codechef.com/problems/BATTERYLOW
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	for(int i =1;i<=t;i++)
	{
	    int x;
	    cin>>x;
	    
	    if(x<=15)
	    {
	        cout<<"YES"<<endl;
	    }
	    else
	    {
	        cout<<"NO"<<endl;
	    }
	    
	}
}
